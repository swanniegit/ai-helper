{
  "name": "role-based-ai-platform-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t role-based-ai-platform .",
    "docker:run": "docker run -p 3000:3000 role-based-ai-platform",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose up --build",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker system prune -f",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
    "logs": "docker-compose logs -f --tail=50 app",
    "logs:debug": "docker-compose -f docker-compose.debug.yml logs -f --tail=50 app",
    "docker:debug": "docker-compose -f docker-compose.debug.yml up --build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.51.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.17",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "14.2.3",
    "openai": "^4.25.0",
    "postgres": "^3.4.7",
    "react": "^18",
    "react-dom": "^19",
    "tailwind-merge": "^2.2.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^19",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
